<div class="card">
  <img src="/img/stories/{{ story.data.image }}" class="card__cover" alt="{{ "Photo of" | i18n(locale) }} {{ story.data.title }}">
  <div class="card__inner">
    <a href="{{ story.url | link(locale) }}" class="breakout-button"><img src="/img/stories/{{ story.data.logo }}" class="card__logo" alt="{{ story.data.title | i18n(locale) }} {{ "Logo" | i18n(locale) }}"></a>
    <p>{{ story.data.overview | i18n(locale) }}</p>
  </div>
</div>